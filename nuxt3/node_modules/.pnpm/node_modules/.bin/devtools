#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/@nuxt+devtools@1.6.4_rollup@4.29.1_vite@5.4.11_@types+node@22.10.2_terser@5.37.0__vue@3.5.13_typescript@5.7.2_/node_modules/@nuxt/devtools/node_modules:/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/@nuxt+devtools@1.6.4_rollup@4.29.1_vite@5.4.11_@types+node@22.10.2_terser@5.37.0__vue@3.5.13_typescript@5.7.2_/node_modules/@nuxt/node_modules:/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/@nuxt+devtools@1.6.4_rollup@4.29.1_vite@5.4.11_@types+node@22.10.2_terser@5.37.0__vue@3.5.13_typescript@5.7.2_/node_modules:/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/@nuxt+devtools@1.6.4_rollup@4.29.1_vite@5.4.11_@types+node@22.10.2_terser@5.37.0__vue@3.5.13_typescript@5.7.2_/node_modules/@nuxt/devtools/node_modules:/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/@nuxt+devtools@1.6.4_rollup@4.29.1_vite@5.4.11_@types+node@22.10.2_terser@5.37.0__vue@3.5.13_typescript@5.7.2_/node_modules/@nuxt/node_modules:/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/@nuxt+devtools@1.6.4_rollup@4.29.1_vite@5.4.11_@types+node@22.10.2_terser@5.37.0__vue@3.5.13_typescript@5.7.2_/node_modules:/home/mikias/Desktop/food-recipe/nuxt3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi
